datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model User {
  id                String @id @default(cuid())
  name              String @default("")
  email             String @unique @default("")
  password          String
  product           User[] @relation("User_product")
  from_User_product User[] @relation("User_product")
}

model Product {
  id                     String             @id @default(cuid())
  name                   String             @default("")
  description            Json               @default("[{\"type\":\"paragraph\",\"children\":[{\"text\":\"\"}]}]")
  productImage_filesize  Int?
  productImage_extension String?
  productImage_width     Int?
  productImage_height    Int?
  productImage_mode      String?
  productImage_id        String?
  attributes             ProductAttribute[] @relation("Product_attributes")
  price                  Decimal?           @map("product_price") @postgresql.Decimal(7, 2)
}

model Attribute {
  id                              String             @id @default(cuid())
  name                            String             @default("")
  from_ProductAttribute_attribute ProductAttribute[] @relation("ProductAttribute_attribute")
}

model ProductAttribute {
  id                      String     @id @default(cuid())
  attribute               Attribute? @relation("ProductAttribute_attribute", fields: [attributeId], references: [id])
  attributeId             String?    @map("attribute")
  value                   String     @unique @default("")
  from_Product_attributes Product[]  @relation("Product_attributes")

  @@index([attributeId])
}